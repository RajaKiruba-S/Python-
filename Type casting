#type casting

x=1        #int
y=2.5      #float
z="3"      #str

print(x)
print(y)
print(z)

y=int(y)
print(y)     #2

print(z*3)   #333(repeating the vaue bcs it is string)

z=int(z)
print(z*3)   #9(mul the value bcs it is int)
print(y*3)   #6(mul the value but return only int value)

x=float(x)
y=float(y)
z=float(z)

print(x)      #1.0
print(y)      #2.5
print(z)      #3.0

print("X is "+x)        #TypeError: can only concatenate str (not "float") to str
print("X is "+str(x))    #X is 1.0
