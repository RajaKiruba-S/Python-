#Math functions

import math

pi=3.14
print(round(pi))              #3
print(math.ceil(pi))          #4(rounded up)
print(math.floor(pi))         #3(rounded down)
print(abs(pi))                #3.14(return only pos value)
print(pow(pi,2))              #9.8596
print(math.sqrt(pi))          #1.772004514666935


x=1
y=2
z=3
print(max(x,y,z))            #3
print(min(x,y,z))            #1
